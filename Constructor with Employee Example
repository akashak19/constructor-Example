package com.ctorEx;

public class Employee {
	
	int eid;
	String ename;
	double esal;
	
	public Employee(int eid, String ename, double esal) {
		super();
		this.eid = eid;
		this.ename = ename;
		this.esal = esal;
	}

	public Employee(int eid) {
		 
		this.eid = eid;
	}

	public Employee(String ename, double esal) {
		 
		this.ename = ename;
		this.esal = esal;
	}

	public Employee(int eid, String ename) {
		 
		this.eid = eid;
		this.ename = ename;
	}
	
	 

}
